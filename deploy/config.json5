        // Viewster 서브에이전트용 (Max 20x 계정)
        "claude-viewster": {
          command: "claude-viewster",
          args: ["-p", "--output-format", "json", "--dangerously-skip-permissions", "--tools", "default"],
          resumeArgs: ["-p", "--output-format", "json", "--dangerously-skip-permissions", "--tools", "default", "--resume", "{sessionId}"],
          output: "json",
          input: "arg",
          modelArg: "--model",
          systemPromptArg: "--append-system-prompt",
          systemPromptWhen: "first",
          sessionArg: "--session-id",
          sessionMode: "always",
          reliability: { watchdog: {
            fresh:  { noOutputTimeoutMs: 300000 },
            resume: { noOutputTimeoutMs: 300000 }
          }}
        },
